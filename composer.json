{
  "name": "lindesbs/contao-forge",
  "type": "contao-bundle",
  "description": "ContaoForge",
  "keywords": [
    "contao"
  ],
  "license": "proprietary",
  "homepage": "https://contao-forge.de/",
  "authors": [
    {
      "name": "ktrion.de",
      "homepage": "https://ktrion.de",
      "email": "develop@ktrion.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4.8"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1",
    "contao/test-case": "^1.4.1",
    "php-http/guzzle6-adapter": "^1.1",
    "phpunit/phpunit": "^7.0",
    "symfony/phpunit-bridge": "^4.1.2",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "doctrine/data-fixtures": "1.0.*",
    "doctrine/dbal": "~2.4",
    "doctrine/orm": "~2.4,>=2.4.5",
    "doctrine/doctrine-bundle": "~1.2"
  },
  "extra": {
    "contao-manager-plugin": "lindesbs\\ContaoForge\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "lindesbs\\ContaoForge\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "lindesbs\\ContaoForge\\": "tests/"
    }
  },
  "support": {
    "docs": "https://ktrion.de/",
    "issues": "https://github.com/lindesbs/contao-forge/issues",
    "source": "https://github.com/lindesbs/contao-forge"
  }
}
